<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="view_stock_move_tree" model="ir.ui.view">
        <field name="name">view.stock.move.tree</field>
        <field name="model">stock.move.ept</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="product_id"/>
                <field name="uom_id"/>
                <field name="source_location_id"/>
                <field name="destination_location_id"/>
                <field name="qty_to_deliver"/>
                <field name="qty_delivered"/>
                <field name="state"/>
                <field name="sale_line_id"/>
                <field name="picking_id"/>
            </tree>
        </field>
    </record>

    <record id="view_stock_move_form" model="ir.ui.view">
        <field name="name">view.stock.move.form</field>
        <field name="model">stock.move.ept</field>
        <field name="arch" type="xml">
            <form string="View Stock Move Form">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="product_id"/>
                        <field name="uom_id"/>
                        <field name="source_location_id"/>
                        <field name="destination_location_id"/>
                        <field name="qty_to_deliver"/>
                        <field name="qty_delivered"/>
                        <field name="state"/>
                        <field name="sale_line_id"/>
                        <field name="picking_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
