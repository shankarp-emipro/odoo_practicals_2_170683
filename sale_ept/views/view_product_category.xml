<?xml version="1.0" encoding="UTF-8" ?>

<odoo>

    <record id="view_product_category_tree4" model="ir.ui.view">
        <field name="name">view.product.category.tree4</field>
        <field name="model">product.category.ept4</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_product_category_form4" model="ir.ui.view">
        <field name="name">view.product.category.form4</field>
        <field name="model">product.category.ept4</field>
        <field name="arch" type="xml">
            <form string="ProductCategoryFormView">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_product_category4" model="ir.actions.act_window">
        <field name="name">Product Category</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">product.category.ept4</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p>Please enter some data</p>
        </field>
    </record>

</odoo>
