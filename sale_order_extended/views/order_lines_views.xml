<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="view_sale_order_extended_order_lines_tree" model="ir.ui.view">
        <field name="name">view.sale.order.extended.order.lines.tree</field>
        <field name="model">sale.order.line</field>
        <field name="arch" type="xml">
            <tree>
                <field name="product_id"/>
            </tree>
        </field>
    </record>

    <record id="action_view_sale_order_lines" model="ir.actions.act_window">
        <field name="name">Reserved Order Lines</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">sale.order.line</field>
        <field name="view_mode">tree</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p>Please enter some data</p>
        </field>
    </record>
</odoo>