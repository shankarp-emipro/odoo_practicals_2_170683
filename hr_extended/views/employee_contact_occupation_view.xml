<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="view_employee_contact_occupation_tree" model="ir.ui.view">
        <field name="name">view.employee.contact.occupation.tree</field>
        <field name="model">employee.contact.occupation</field>
        <field name="arch" type="xml">
            <tree string="Contact Occupation Tree">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_employee_contact_occupation_form" model="ir.ui.view">
        <field name="name">view.employee.contact.occupation.form</field>
        <field name="model">employee.contact.occupation</field>
        <field name="arch" type="xml">
            <form string="Contact Occupation Form">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_employee_contact_occupation" model="ir.actions.act_window">
        <field name="name">Contact Occupation</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">employee.contact.occupation</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">Enter some data</p>
        </field>
    </record>

</odoo>